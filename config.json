{
    "name": "FasterRCNN_Res50_FPN",
    "n_gpu": 1,
    "fold": 0,
    "arch": {
        "type": "FasterRCNN",
        "args": {
            "num_classes": 11
        }
    },
    "collate_fn": {
        "type": "fasterRCNN_collate_fn",
        "args": {}
    },
    "train_aug": {
        "type": "train_augmentation",
        "args": {}
    },
    "valid_aug": {
        "type": "valid_augmentation",
        "args": {}
    },
    "dataset": {
        "type": "CustomDataset",
        "args": {
            "annotation": "/opt/ml/dataset/train.json",
            "data_dir": "/opt/ml/dataset"
        }
    },
    "data_loader": {
        "type": "CustomDataLoader",
        "args": {
            "batch_size": 32,
            "shuffle": false,
            "validation_split": 0,
            "num_workers": 0
        }
    },
    "valid_data_loader": {
        "type": "CustomDataLoader",
        "args": {
            "batch_size": 1,
            "shuffle": false,
            "validation_split": 0,
            "num_workers": 0
        }
    },
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.005,
            "weight_decay": 0.0005,
            "momentum": 0.9
        }
    },
    "loss": "nll_loss",
    "metrics": [],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 3,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": true
    }
}